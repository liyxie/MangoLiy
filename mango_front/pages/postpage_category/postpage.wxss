  text-align: left;
  overflow: hidden;
  -webkit-line-clamp: 6;
  -webkit-box-orient: vertical;
  letter-spacing: 5rpx;
}

.main_bottom {
  margin-bottom: 10rpx;
}

.main_middle_image {
  display: flex;
  flex-direction: row;
